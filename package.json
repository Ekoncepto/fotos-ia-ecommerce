{
  "name": "fotos-ia-ecommerce",
  "private": true,
  "scripts": {
    "dev": "./node_modules/.bin/turbo dev",
    "build": "./node_modules/.bin/turbo build",
    "start": "./node_modules/.bin/turbo start",
    "lint": "./node_modules/.bin/turbo lint",
    "test": "./node_modules/.bin/turbo test",
    "migrate": "supabase db push --db-url \"$DB_POOLER_URL\"",
    "postinstall": "rm -rf apps/web/node_modules/@types"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "callsites": "^4.2.0",
    "dotenv-cli": "^10.0.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.3",
    "supabase": "latest",
    "turbo": "latest"
  },
  "packageManager": "npm@11.6.0",
  "dependencies": {
    "@types/react": "18.3.24",
    "@types/react-dom": "18.3.7",
    "busboy": "^1.6.0",
    "dompurify": "^3.2.6",
    "uuid": "^12.0.0"
  },
  "overrides": {
    "@types/react": "18.3.24"
  }
}
